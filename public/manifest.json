{
  "manifest_version": 2,
  "name": "收藏夹",
  "description": "收藏夹网址导航",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "main.html"
  },
  "icons":{
    "16": "icon.png"
  },
  "background": {
    "page": "background.html"
  },
  "omnibox": { "keyword" : "zyy" },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "contextMenus"
  ]
}